<br>
<table class="table" style="width:80%; text-align:center; margin: 0 auto; ">
  <thead class="thead-dark">
    <tr>
      <th>Week</th>
      <th>Picked Team</th>
      <th>Date Picked</th>
      <th>Outcome</th>
    </tr>
  </thead>
  <tbody>
    <% @picks.each do |p|%>
    <tr>
      <td>Week <%= p.week.id%></td>
      <td><%= p.team.name%></td>
      <td><%= p.updated_at%></td>
    </tr>
    <%end%>
  </tbody>
</table>